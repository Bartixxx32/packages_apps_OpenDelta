android_app {
    name: "OpenDelta",
    srcs: ["src/**/*.java"],
    resource_dirs: ["res"],
    defaults: [
        "platform_app_defaults",
        "SettingsLibDefaults",
    ],
    static_libs: [
        "androidx.core_core",
        "androidx.cardview_cardview",
        "androidx.preference_preference",
    ],
    certificate: "platform",
    privileged: true,
    optimize: {
        enabled: false,
    },
    platform_apis: true,
}
